// what is the value of the variable ifStatement, leave your answer in the console. 

let ifStatement; 
let value = 5; 

if(value < 5)
{
    ifStatement = true;
}

else{
    ifStatement = false; 
}
console.log("The value is 5. Meaning the statement can be neither true or false since both sides are 5. The ifStatement is \
    if 5 is less than 5 than it is true, if not than it is false.")
